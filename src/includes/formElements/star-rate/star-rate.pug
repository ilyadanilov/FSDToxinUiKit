mixin starRate
  .star-rate
    input.star-rate__check(name="myrating" type="radio" value="5")
    span.star-rate__star ☆
    input.star-rate__check(name="myrating" type="radio" value="4")
    span.star-rate__star ☆
    input.star-rate__check(name="myrating" type="radio" value="3")
    span.star-rate__star ☆
    input.star-rate__check(name="myrating" type="radio" value="2")
    span.star-rate__star ☆
    input.star-rate__check(name="myrating" type="radio" value="1")
    span.star-rate__star ☆
